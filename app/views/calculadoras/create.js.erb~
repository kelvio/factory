$('#total').html('');
$('#total').html('<%= number_to_currency @cheque.valor %> a <%= number_to_percentage @cheque.taxa_juros, :precision => 0 %>  = <%= number_to_currency @cheque.valor_pago_factory %> (com o vencimento para <%= @cheque.vencimento %>)');
$('#valor').focus();
$('#calculadora_form')[0].reset();
