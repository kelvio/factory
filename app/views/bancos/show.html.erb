<h1>Banco</h1>
<div class="box-left">
<p id="notice"><%= notice %></p>

<p>
  <b>Nome:</b>
  <%= @banco.nome %>
</p>

<p>
  <b>Codigo:</b>
  <%= @banco.codigo %>
</p>

<p>
  <b>Site:</b>
  <%= @banco.site %>
</p>
</div>
<br />
<input type="button" onclick="location.href='<%= edit_banco_path(@banco) %>'" value="<%= t('banco.show.edit_link_label') %>" />
<input type="button" onclick="location.href='<%= bancos_path %>'" value="<%= t('banco.show.back_link_label') %>" />

